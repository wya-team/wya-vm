    "@wya/ps": "^1.0.2",
    "@wya/sass": "^1.1.10",
    "@wya/utils": "^1.5.1",
    "@wya/vc": "^1.8.4",
    "vue": ">=2.6.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "esw --fix",
      "git add"
    ],
    "*.md": [
      "esw --ext '.md' --fix",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ]
