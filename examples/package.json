{
  "name": "vue-env",
  "version": "0.1.0",
  "description": "basic env for vue",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run prompt && npm run all",
    "serve": "node --max_old_space_size=8192 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config  ./config/webpack.config.dev.js --hot",
    "dist": "npm run remove:dist && npm run lint && node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack --config ./config/webpack.config.dist.js",
    "build": "cross-env UGLIFY_JS=true npm run dist -- -p && npm run copy",
    "build:dev": "cross-env UGLIFY_JS=false npm run dist && npm run copy",
    "remove:dist": "rm -rf ./dist",
    "clear": "rm -rf ./node_modules/.cache",
    "copy": "echo 'end'",
    "lint": "esw config ./src/pages --ext .vue,.js --color",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --fix --watch",
    "lint:style": "stylelint --fix --syntax scss src",
    "mock": "cd ./src/mock && npm run start",
    "prompt": "node ./config/prompt.config.js",
    "all": "cross-env BB=development npm-run-all --parallel --race --continue-on-error lint:watch serve",
    "lint-staged": "lint-staged",
    "pre-commit:install": "cp .hooks/* .git/hooks/",
    "create": "wya add --config ./config/toolkit.config.js",
    "test": "jest --config ./tests/config/jest.config.js"
  },
  "author": "wya-team",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@wya/http": "^1.2.0",
    "@wya/ps": "^1.0.2",
    "@wya/qrcode": "^1.0.2",
    "@wya/sass": "^1.1.10",
    "@wya/socket": "^1.0.3",
    "@wya/utils": "^1.5.4",
    "@wya/vc": "^2.0.0-alpha.26",
    "@wya/vm": "^2.0.0-alpha.45",
    "ali-oss": "^6.1.0",
    "fast-xml-parser": "^3.12.12",
    "js-md5": "^0.7.3",
    "moment": "^2.24.0",
    "vue": "^2.6.7",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-function-bind": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/plugin-transform-block-scoping": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@wya/vc-loader": "^0.3.4",
    "assets-webpack-plugin": "^3.9.10",
    "autoprefixer": "^9.6.5",
    "babel-eslint": "^10.0.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.2",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "copy-webpack-plugin": "^5.0.4",
    "cp-cli": "^2.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.10.0",
    "enzyme-to-json": "^3.4.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "eslint-watch": "^6.0.1",
    "file-loader": "^4.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^8.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "inquirer": "^7.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "precss": "^4.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.1",
    "shelljs": "^0.8.3",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-webpack-plugin": "^1.0.2",
    "url-loader": "^2.2.0",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "~4.41.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2"
  },
  "lint-staged": {
    "{src,config}/**/*.{js,jsx,vue}": [
      "esw --fix",
      "git add"
    ],
    "*.md": [
      "esw --ext '.md' --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix --syntax scss",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ]
}
